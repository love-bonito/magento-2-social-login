{
  "name": "mageplaza/magento-2-social-login",
  "description": "Magento 2 Social Login extension is designed for quick login to your Magento 2 store without procesing complex register steps",
  "require": {
    "mageplaza/module-core": "^1.4.5",
    "mageplaza/module-smtp": "^4.0.0",
    "mageplaza/module-google-recaptcha": "^4.0.0",
    "mageplaza/module-two-factor-authentication": "dev-v4-php-8.1",
    "hybridauth/hybridauth": "dev-v2-php-8.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "type": "magento2-module",
  "version": "4.1.3",
  "license": "proprietary",
  "authors": [
    {
      "name": "Mageplaza",
      "email": "support@mageplaza.com",
      "homepage": "https://www.mageplaza.com",
      "role": "Technical Support"
    }
  ],
  "keywords": [
    "magento 2",
    "social login",
    "magento 2 social login",
    "facebook login",
    "Google Login",
    "Instagram Login",
    "LinkedIn Login",
    "Microsoft Live Login",
    "Twitter Login",
    "Yahoo Login",
    "FourSquare Login",
    "Amazon Login",
    "Login",
    "mageplaza",
    "google",
    "facebook",
    "twitter",
    "social proof"
  ],
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Mageplaza\\SocialLogin\\": ""
    }
  },
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/tunght13488/hybridauth.git"
    },
    {
      "type": "github",
      "url": "https://github.com/tunght13488/php-graph-sdk.git"
    },
    {
      "type": "github",
      "url": "https://github.com/tunght13488/magento-2-two-factor-authentication.git"
    },
    {
      "type": "github",
      "url": "https://github.com/tunght13488/GoogleAuthenticator.git"
    }
  ],
  "minimum-stability": "dev"
}
